#1 - usuario do banco
spring.datasourcer.username = root
#2 - senha do banco
spring.datasource.password = bananajava
#3 - string de conexao com a base criada
spring.datasource.url = jdbc:mysql://localhost:3306/projeto?useTimezone=true&serverTimezone=UTC
#4 - dialeto
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL5Dialect

#parametros opcionais
spring.jpa.show_sql = true

#delegar ao JPA a eventual crição dos 
spring.jpa.hibernate.ddl-auto = update